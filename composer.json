{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "asbiin/laravel-webauthn": "^4.6",
        "aws/aws-sdk-php": "^3.325",
        "bavix/laravel-wallet": "^11.3",
        "dipeshsukhia/laravel-country-state-city-data": "^2.0",
        "fdbatista/bitso-php": "*",
        "guzzlehttp/guzzle": "*",
        "laravel/framework": "^11.9",
        "laravel/jetstream": "^5.3",
        "laravel/sanctum": "^4.0",
        "laravel/telescope": "^5.5",
        "laravel/tinker": "^2.9",
        "league/flysystem-aws-s3-v3": "^3.29",
        "nwidart/laravel-modules": "^11.1",
        "paragonie/sodium_compat": "^2.1",
        "pomelopay/pomelopay-connect-php": "^2.1",
        "pragmarx/google2fa-laravel": "^2.2",
        "spatie/laravel-permission": "^6.9",
        "spatie/laravel-webhook-server": "^3.8",
        "stephenjude/laravel-wallet": "^2.0",
        "symfony/http-client": "^7.1",
        "tightenco/ziggy": "^2.3",
        "towoju5/advcash": "^0.1.0",
        "towoju5/bitnob": "^1.2",
        "towoju5/laravel-azure-mailer": "^1.1",
        "towoju5/laravel-referral": "^1.1",
        "towoju5/localpayments": "^1.2",
        "towoju5/plans": "^1.1",
        "tymon/jwt-auth": "*",
        "yadahan/laravel-authentication-log": "^1.7",
        "yajra/laravel-datatables-buttons": "^11.2",
        "yajra/laravel-datatables-html": "^11.6",
        "yajra/laravel-datatables-oracle": "^11.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "pestphp/pest": "^3.5",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "files": [
            "app/Helpers/helper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        },
        "merge-plugin": {
            "include": [
                "Modules/*/composer.json"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
